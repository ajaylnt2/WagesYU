                                                            <table class="table" id="budgetedwaterCostTable" data-page-size="5" data-page-list="[5, 10, 25, 50, 100, ALL]" data-mobile-responsive="true"></table>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!--Actual Water Cost starts here-->
                                                <br />
                                                <div class="col-xs-3">
                                                    <input type="button" name="Save Details" value="Save Details" id="idWaterBudgetedSaveDetails" class="btn btn-success" onclick="saveBudgetedDetails(1)" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="airTab" class="tab-pane fade">
