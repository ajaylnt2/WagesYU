
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Shared/PlantSetup/Budgeted/ConsumptionBudgeted.js"></script>

<!-- Main Div Starts Here-->
<div id="page-wrapper">
    <br />
    <div class="panel panel-primary">
        <div class="panel-heading" id="">
            <h4 class="panel-title">
                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" id="wageTitle">
                    Budgeted Details
                </a>
            </h4>
        </div>
        <div class="panel-body">
            <div class="container-fluid">
                <div class="col-lg-12">
                    <!-- Year Section Starts-->
                    <div class="panel panel-primary">
                        <div class="panel-body">
                            <div class="form-group">
                                <label for="year" class="col-sm-1 col-xs-2  col-lg-2  col-md-2 control-label inlineCSS">Year</label>
                                <div class="col-sm-4 ">
                                    <input id="budgetedConsumptionYear" type="text" class="form-control" value=@DateTime.UtcNow.Year onkeypress="return isNumber(event)">
                                </div>
                                <div class="col-sm-3 col-lg-4 col-xs-4 col-md-4 yearTextField">
                                    <input type="button" name="Get Details" value="Fetch Details" id="idbudgetedConsumptionFetchDetails" class="btn btn-info" onclick="fetchDetails()" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Year Section Ends-->

                    <div class="panel panel-primary">
                        <div class="panel-body">
                            <br />
                            <div class="panel panel-default">
                                <div class="panel-body">
                                    <label for="usdExchangeRate" class="col-sm-1 col-xs-2  col-lg-2  col-md-2 control-label inlineCSS">USD Exchange Rate</label>
                                    <div class="col-sm-4 ">
                                        <input id="usdExchangeRate" type="number" class="form-control" value="" onkeypress="return isNumber(event)" min="0"/>
                                    </div>
                                    <div class="col-xs-3">
                                        <input type="button" name="Save" value="Save Exchange Rate" id="idSaveUSDrate" class="btn btn-success" onclick="saveUSDRate()" />
                                    </div>
                                </div>
                            </div>
                            <br />
                            <ul class="nav nav-tabs">
                                <li id="water" class="active"><a data-toggle="tab" href="#waterTab" onclick="loadBudgetedGrids(1)">Water</a></li>
                                <li id="air"><a data-toggle="tab" href="#airTab" onclick="loadBudgetedGrids(2)">Air</a></li>
                                <li id="gas"><a data-toggle="tab" href="#gasTab" onclick="loadBudgetedGrids(3)">Gas</a></li>
                                <li id="electricity"><a data-toggle="tab" href="#electricityTab" onclick="loadBudgetedGrids(4)">Electricity</a></li>
                                <li id="steam"><a data-toggle="tab" href="#steamTab" onclick="loadBudgetedGrids(5)">Steam</a></li>
                                <li id="production"><a data-toggle="tab" href="#productionTab" onclick="loadProduction()">Production</a></li>
                                <li id="solidwaste"><a data-toggle="tab" href="#solidwasteTab" onclick="loadsolidWasteBudgetedGrid()">Solid Waste</a></li>
                            </ul>
                            <!--Tabs Panes Starts-->

                            <div class="tab-content">
                                <div id="waterTab" class="tab-pane fade in active">
                                    <br />
                                    <div class="panle panel-default">
                                        <div class="panel-body">
                                            <div class="col-lg-12" id="waterConsumptionDiv">
                                                <!--Actual Water Consumption starts here-->
