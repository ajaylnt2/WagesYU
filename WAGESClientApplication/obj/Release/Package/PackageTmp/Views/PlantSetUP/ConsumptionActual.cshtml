                            <div class="form-group">
                                <label for="year" class="col-sm-1 col-xs-2  col-lg-2  col-md-2 control-label inlineCSS">Year</label>
                                <div class="col-sm-4 ">
                                    <input id="actualConsumptionYear" type="text" class="form-control" value=@DateTime.UtcNow.Year onkeypress="return isNumber(event) ">
                                </div>
                                <div class="col-sm-3 col-lg-4 col-xs-4 col-md-4 yearTextField">
                                    <input type="button" name="Get Details" value="Fetch Details" id="idactualConsumptionFetchDetails" class="btn btn-info" onclick="fetchDetails()" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Year Section Ends-->
                    <div class="panel panel-primary">
                        <div class="panel-body">
                            <ul class="nav nav-tabs">
                                <li class="active" id="water"><a data-toggle="tab" href="#waterTab" onclick="loadGrids(1)">Water</a></li>
                                <li id="air"><a data-toggle="tab" href="#airTab" onclick="loadGrids(2)">Air</a></li>
                                <li id="gas"><a data-toggle="tab" href="#gasTab" onclick="loadGrids(3)">Gas</a></li>
