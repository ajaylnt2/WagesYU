                                                    <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" id="wageTitle" href="#consumptionCollapseMenu">
                                                        Consumption
                                                    </a>
                                                </h4>
                                            </div>
                                            <div id="consumptionCollapseMenu" +@i class="panel-collapse collapse in">
                                                <div class="panel-body">
                                                    <table class="table" id="waterConsumptionTable" data-mobile-responsive="true"></table>
                                                </div>
                                            </div>
                                        </div>
                                        <!--Actual Water Consumption ends here-->
                                        <!--Actual Water Cost starts here-->
                                        <div class="panel panel-primary">
                                            <div class="panel-heading" id="">
                                                <h4 class="panel-title">
                                                    <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" id="wageTitle" href="#costCollapseMenu">
                                                        Cost
                                                    </a>
                                                </h4>
                                            </div>
                                            <div id="costCollapseMenu" +@i class="panel-collapse collapse in">
                                                <div class="panel-body">
                                                    <table class="table" id="waterCostTable" data-page-size="5" data-page-list="[5, 10, 25, 50, 100, ALL]" data-mobile-responsive="true"></table>
                                                </div>
                                            </div>
                                        </div>
                                        <!--Actual Water Cost starts here-->
                                        <br />
                                        <div class="col-xs-3">
                                            <input type="button" name="Save Details" value="Save Details" id="idWaterActualSaveDetails" class="btn btn-success" onclick="saveActuals(1)" />
                                        </div>
                                    </div>
                                </div>
                                <div id="airTab" class="tab-pane fade">
                                    <br />
                                    <div class="col-lg-12" id="airConsumptionDiv">
