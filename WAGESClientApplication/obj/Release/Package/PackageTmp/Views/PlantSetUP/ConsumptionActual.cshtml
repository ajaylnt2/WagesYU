
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Shared/PlantSetup/Actuals/ConsumptionActuals.js"></script>

<!-- Main Div Starts Here-->
<div id="page-wrapper">
    <br />
    <div class="panel panel-primary">
        <div class="panel-heading" id="">
            <h4 class="panel-title">
                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" id="wageTitle" href="#consumptionCollapseMenu">
                    Actual Details
                </a>
            </h4>
        </div>
        <div class="panel-body">
            <div class="container-fluid">
                <div class="col-lg-12">
                    <!-- Year Section Starts-->
