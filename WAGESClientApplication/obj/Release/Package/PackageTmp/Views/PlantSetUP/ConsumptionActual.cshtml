                                <li id="production"><a data-toggle="tab" href="#productionTab" onclick="loadProduction()">Production</a></li>
                                <li id="solidwase"><a data-toggle="tab" href="#solidwasteTab" onclick="loadSolidWasteGrid()">Solid Waste</a></li>
                            </ul>
